ue":"<__PLACEHOLDER_VALUE__Sales Slack channel ID__>"},"otherOptions":{}},"typeVersion":2.3},{"id":"5895cc01-9106-46c9-84ee-f91e3fc68907","name":"Notify Support Channel","type":"n8n-nodes-base.slack","position":[1664,304],"webhookId":"0a38a6d2-0f3f-40ca-b9b2-174e2c158554","parameters":{"text":"=New support request from {{ $json.name }} ({{ $json.email }}). Interest: {{ $json.interest }}","select":"channel","channelId":{"__rl":true,"mode":"id","value":"<__PLACEHOLDER_VALUE__Support Slack channel ID__>"},"otherOptions":{}},"typeVersion":2.3}],"active":false,"pinData":{},"settings":{"executionOrder":"v1"},"versionId":"893f4e6e-3281-45bf-8c87-a5809a6b0c3e","connections":{"Typeform Trigger":{"main":[[{"node":"Set Response Fields","type":"main","index":0}]]},"Route by Interest":{"main":[[{"node":"Notify Sales Channel","type":"main","index":0}],[{"node":"Notify Support Channel","type":"main","index":0}],[{"node":"Send Confirmation Email","type":"main","index":0}]]},"Set Response Fields":{"main":[[{"node":"Log to Google Sheets","type":"main","index":0}]]},"Log to Google Sheets":{"main":[[{"node":"Route by Interest","type":"main","index":0}]]}}}